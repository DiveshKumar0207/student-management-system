<!DOCTYPE html>
<html lang="en">

<head>
    {{> head}}
</head>

<body>
  <!-- animation -->
    {{> animation}}
    <!-- animation ends -->

    <!-- navbar -->
    {{> navbar}}
    {{!-- navbar-end --}}
   

    <!-- sidebar-nav -->
    {{> I_sidebar}}
    {{!-- sidebar end --}}

    <div class="container">
        <section> Zillionsoftech</section>
        <form>
            <ul class="flex-outer">
                <li>
                    <label for="first-name">First Name</label>
                    <input type="text" id="first-name" placeholder="Enter your first name here" required>
                </li>
                <li>
                    <label for="last-name">Last Name</label>
                    <input type="text" id="last-name" placeholder="Enter your last name here">
                </li>
                <li>
                    <label for="email">Email</label>
                    <input type="email" id="email" placeholder="Enter your email here" required>
                </li>
                <li>
                    <label for="phone">Phone</label>
                    <input type="tel" id="phone" placeholder="Enter your phone here" required>
                </li>
                <li>
                    <label for="User_location">Location</label>
                    <input type="text" id="User_location" placeholder="Enter your location" required>
                </li>

                <li>
                    <label for="message">Inquiry msg</label>
                    <textarea rows="6" id="message"
                        placeholder="Enter your message here Or inquiry about course you interested in"
                        required></textarea>
                </li>

                <li>
                    <p>Age</p>
                    <ul class="flex-inner">
                        <li>
                            <input type="radio" id="ten-to-nineteen" name="age_Enquiry" checked>
                            <label for="twenty-to-twentynine">10-19</label>
                        </li>
                        <li>
                            <input type="radio" id="twenty-to-twentynine" name="age_Enquiry">
                            <label for="twenty-to-twentynine">20-29</label>
                        </li>
                        <li>
                            <input type="radio" id="thirty-to-thirtynine" name="age_Enquiry">
                            <label for="thirty-to-thirtynine">30-39</label>
                        </li>
                        <li>
                            <input type="radio" id="fourty-to-fourtynine" name="age_Enquiry">
                            <label for="fourty-to-fourtynine">40-49</label>
                        </li>

                        <li>
                            <input type="radio" id="other" name="age_Enquiry">
                            <label for="other">Other</label>
                        </li>
                    </ul>
                </li>
                <li>
                    <button type="submit">Submit</button>
                </li>
            </ul>
        </form>
    </div>

      <script src="js/jscript.js"></script>    
</body>

</html>